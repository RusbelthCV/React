    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-preset-nano-react-app": "^0.1.0",
    "parcel-bundler": "^1.12.3"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
